{
  "name": "tigerspack",
  "version": "0.2.1",
  "description": "The fastest UI framework for React",
  "main": "dist/index.js",
  "keywords": [
    "tigerspack",
    "ui-framework",
    "ui-components",
    "ui-kit",
    "react",
    "react-components",
    "design-system"
  ],
  "scripts": {
    "build": "webpack --mode production",
    "start": "NODE_ENV=development webpack-dev-server --open",
    "lint": "eslint --report-unused-disable-directives src",
    "fix": "eslint --fix --report-unused-disable-directives src",
    "test": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "setup": "cp ./.github/hooks/pre-commit ./.git/hooks/pre-commit && chmod +x ./.git/hooks/pre-commit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tigerspack/tigerspack.git"
  },
  "author": "awibox",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tigerspack/tigerspack/issues"
  },
  "homepage": "https://tigerspack.com",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "coveralls": "^3.0.9",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-react": "^7.17.0",
    "html-webpack-plugin": "^4.2.0",
    "jest": "^26.2.1",
    "jest-enzyme": "^7.1.2",
    "jsdom": "^16.2.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "!src/**/index.{js,jsx,mjs}",
      "!src/index.js",
      "!src/utils/**/*.{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "snapshotResolver": "<rootDir>/spec/jestSnapshotResolver",
    "setupFilesAfterEnv": [
      "<rootDir>/node_modules/jest-enzyme/lib/index.js",
      "<rootDir>/spec/jestSetupTests.js",
      "<rootDir>/spec/jestSetupJsDom.js"
    ]
  }
}
